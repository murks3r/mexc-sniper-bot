{
  "name": "mexc-sniper-bot",
  "version": "0.1.0",
  "description": "MEXC cryptocurrency trading bot with pattern detection and automated sniping execution",
  "scripts": {
    "postinstall": "echo 'Dependencies installed'",
    "dev": "next dev --webpack -p 3008",
    "dev:full": "npm run dev & npx inngest-cli dev",
    "dev:inngest": "npx inngest-cli dev",
    "dev:turbo": "next dev -p 3008",
    "prebuild": "node scripts/ensure-lightningcss.js",
    "build": "next build --webpack",
    "build:webpack": "next build --webpack",
    "test:integration": "USE_REAL_SUPABASE=true vitest --run",
    "start": "next start",
    "lint": "npx biome check --write",
    "lint:fix": "npx biome check --fix --unsafe",
    "format": "npx biome format --write .",
    "format:check": "npx biome format --check .",
    "organize-imports": "npx biome check --fix --unsafe --only=organizeImports",
    "sync-format": "npm run format && npm run organize-imports",
    "type-check": "tsc --noEmit",
    "test": "vitest --run",
    "test:watch": "vitest --watch",
    "test:coverage": "vitest --coverage",
    "db:migrate": "drizzle-kit migrate",
    "db:push": "drizzle-kit push",
    "db:studio": "drizzle-kit studio",
    "db:generate": "drizzle-kit generate"
  },
