    "npm": ">=10.0.0"
  },
  "devDependencies": {
    "@biomejs/biome": "^2.0.6",
    "@biomejs/cli-linux-x64": "^2.0.6",
    "@next/bundle-analyzer": "^15.3.4",
    "@playwright/test": "^1.53.2",
    "@rollup/rollup-linux-x64-gnu": "^4.44.1",
    "@tailwindcss/postcss": "^4.1.11",
    "@testing-library/jest-dom": "^6.6.3",
    "@testing-library/react": "^16.3.0",
    "@types/node": "^24.0.10",
    "@types/pg": "^8.15.4",
