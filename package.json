    "@vitest/ui": "^3.2.4",
    "istanbul-lib-coverage": "^3.2.1",
    "jsdom": "^26.1.0",
    "pg": "^8.16.3",
    "testcontainers": "^11.1.0",
    "tsx": "^4.20.3",
    "typescript": "^5.8.3",
    "undici": "^7.16.0",
    "vite-tsconfig-paths": "^5.1.4",
    "vitest": "^3.2.4"
  },
